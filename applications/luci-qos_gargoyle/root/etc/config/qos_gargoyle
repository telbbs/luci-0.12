
config qos_gargoyle 'config'
	option enabled '0'

config upload 'upload'
	option total_bandwidth '1024'
	option monenabled 'true'
	option default_class 'uclass_2'

config download 'download'
	option monenabled 'true'
	option default_class 'dclass_2'
	option total_bandwidth '12000'

config upload_class 'uclass_1'
	option name 'VoIP'
	option percent_bandwidth '1'
	option min_bandwidth '80'
	option minRTT 'No'

config upload_class 'uclass_2'
	option name 'Fast'
	option percent_bandwidth '80'
	option min_bandwidth '512'
	option max_bandwidth '1000'
	option minRTT 'Yes'

config upload_class 'uclass_3'
	option name 'Slow'
	option percent_bandwidth '1'
	option min_bandwidth '128'
	option max_bandwidth '256'
	option minRTT 'No'

config upload_class 'uclass_4'
	option name 'Normal'
	option percent_bandwidth '18'
	option min_bandwidth '256'
	option max_bandwidth '512'
	option minRTT 'No'

config upload_rule 'upload_rule_100'
	option test_order '100'
	option class 'uclass_3'
	option min_pkt_size '1'
	option max_pkt_size '51200'
	option ipp2p 'ipp2p'

config upload_rule 'upload_rule_200'
	option class 'uclass_4'
	option test_order '200'
	option dstport '80'
	option connbytes_kb '256'
	option ipp2p 'ipp2p'

